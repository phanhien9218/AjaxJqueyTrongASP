@{
    ViewBag.Title = "Home Page";
}
@section scripts{
    <script src="~/Scripts/Plugins/mustache.min.js"></script>
    <script src="~/Scripts/Controller/homeController.js"></script>
}
<div class="row">
    <table class="table">
        <thead>
            <tr>
                <td>ID</td>
                <td>Name</td>
                <td>Salary</td>
                <td>Status</td>
            </tr>
        </thead>
        <tbody id="tblData"></tbody>
    </table>
    <script id="data-template" type="x-tmpl-mustache">
        <tr>
            <td>{{ID}}</td>
            <td>{{Name}}</td>
            <td><input type="number" class="txtSalary" data-id="{{ID}}" value="{{Salary}}" /></td>
            <td>{{{Status}}}</td>
        </tr>
    </script>
</div>
